/*Question 1.1*/
make_pairs(U,[],[]).
make_pairs(U,[X|Y],Res):-
    make_pairs(U,Y,Res1),
    \==(U,X),
    append(Res1,[likes(U,X),likes(X,U)],Res).
make_pairs(U,[U|Y],Res):-
    make_pairs(U,Y,Res1),
    append(Res1,[likes(U,U)],Res).
    

make_all_pairs([],[]).
make_all_pairs([F|R],Res):-
